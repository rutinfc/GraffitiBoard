# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'GraffitiBoard' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for GraffitiBoard
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'GoogleSignIn'
  pod 'AFNetworking'
  pod 'NXDrawKit'
  pod 'RSKImageCropper'
  
  target 'GraffitiBoardTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    pod 'GoogleSignIn'
    pod 'AFNetworking'
    pod 'NXDrawKit'
    pod 'RSKImageCropper'
  end

end
